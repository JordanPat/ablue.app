    "docker-run": "docker run -p 5000:5000 your-app-name",
    "docker-stop": "docker stop $(docker ps -q --filter ancestor=your-app-name)",
    "docker-clean": "docker rm $(docker ps -a -q --filter ancestor=your-app-name)"
